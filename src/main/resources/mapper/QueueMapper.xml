<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.limengxiang.jtq.queue.DefaultQueue">
    <insert id="push" parameterType="com.limengxiang.jtq.message.DefaultMessage">
        INSERT INTO jtq_queue (msg_id,body,schedule,status,receive_at,takeaway_at)
        VALUES (#{msgId},#{body},#{schedule},#{status},#{receiveAt},#{takeawayAt})
    </insert>
    <select id="get" resultType="com.limengxiang.jtq.message.DefaultMessage">
        SELECT * FROM jtq_queue WHERE msg_id = #{id}
    </select>
</mapper>
